# crypto_test executable

check_PROGRAMS += crypto_test

crypto_test_includes = \
	../emulator/include/tee_internal_api.h \
	../emulator/include/tee_logging.h \
	../emulator/internal_api/tee_object_handle.h

crypto_test_SOURCES = ${crypto_test_include} \
	internal_api/crypto_test.c

crypto_test_CFLAGS = ${AM_CFLAGS} \
	-iquote $(top_srcdir)/../emulator/include

crypto_test_LDFLAGS = ${AM_LDFLAGS} \
	-lcrypto

crypto_test_LDADD = ../emulator/libInternalApi.la

